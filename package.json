  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.14.1",
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/generator": "^3.26.0",
    "@sap-cloud-sdk/http-client": "^3.26.0",
    "@sap-cloud-sdk/odata-v2": "^3.26.0",
    "@sap-cloud-sdk/resilience": "^3.26.0",
    "@sap/cds": "^8",
    "@sap/cds-mtxs": "^2",
    "@sap/cds-sqlite": "^1.24.1",
    "@sap/xsenv": "^5.4.0",
    "@sap/xssec": "^4",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "express": "^4",
    "express-fileupload": "^1.5.1",
    "hdb": "^0.19.12",
    "rimraf": "^6.0.1",
    "sqlite": "^5.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.9.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8.7.1"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "fiori": {
      "preview": true
    },
    "server": {
      "index": true
    },
    "requires": {
      "db": {
        "kind": "hana",
        "vcap": {
          "name": "pdfsave-db"
        },
        "deploy-format": "hdbtable"
      },
      "auth": {
        "kind": "mocked"
      },
      "cors": {
        "enabled": true
      },
      "destinations": true,
      "html5-repo": true,
      "workzone": true,
      "[production]": {
        "auth": {
          "kind": "jwt"
        },
        "db": {
          "kind": "hana",
          "vcap": {
            "name": "pdfsave-db"
          }
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
