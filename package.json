{
  "name": "pdfsave",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.14.1",
    "@cap-js/hana": "^1.9.1",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/generator": "^3.26.0",
    "@sap-cloud-sdk/http-client": "^3.26.4",
    "@sap-cloud-sdk/odata-v2": "^3.26.4",
    "@sap-cloud-sdk/resilience": "^3.26.0",
    "@sap/cds": "^8",
    "@sap/cds-sqlite": "^1.24.1",
    "@sap/xsenv": "^5.4.0",
    "@sap/xssec": "^4",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "hdb": "^0.19.12",
    "jszip": "^3.10.1",
    "sqlite": "^5.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.9.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8.7.1",
    "@types/chai": "^5.2.2",
    "chai": "^5.2.1",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5",
    "ui5-middleware-simpleproxy": "^3.4.3"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy pdfsave --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "fiori": {
      "preview": true
    },
    "server": {
      "index": true
    },
    "requires": {
      "spa_process_destination": {
        "kind": "destination",
        "credentials": {
          "destination": "spa_process_destination"
        }
      },
      "pdfsave-destination": {
        "kind": "destination",
        "credentials": {
          "destination": "pdfsave-destination"
        }
      },
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/API_BUSINESS_PARTNER"
      },
      "sql": {
        "native_hana_associations": false
      }
    }
  }
}
